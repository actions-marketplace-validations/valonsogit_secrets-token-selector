{
  "name": "secrets-token-selector",
  "version": "0.0.1",
  "private": true,
  "description": "Test action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package",
    "issue": "curl -X POST -H \"Authorization: Bearer $GITHUB_TOKEN\" -H \"Accept: application/vnd.github.v3+json\" https://api.github.com/repos/valonsogit/lete-action/issues -d \"{\\\"title\\\": \\\"Test at $(date +\"%H:%M:%S\")\\\" }\"",
    "refresh": "npm run all && ( git add . && git status; git commit -m \"Dev\";git tag -d v0.0.1; git push --delete origin v0.0.1; git tag -a -m \"Dev\" v0.0.1 && git push --follow-tags && npm run issue )"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.28.0",
    "eslint-plugin-github": "^4.4.1",
    "prettier": "2.8.0",
    "typescript": "^4.9.3"
  }
}